{
  "name": "angular-boilerplate",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:es": "ng serve -c=es --port 4201",
    "build": "ng build",
    "build:prod": "ng build --prod --build-optimizer --aot --stats-json",
    "build:i18n": "ng build --prod --build-optimizer --aot --stats-json --localize",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "i18n:extract": "ng extract-i18n --output-path locale --ivy",
    "analyze": "webpack-bundle-analyzer dist/angular-boilerplate/stats.json"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged --relative"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@angular/animations": "~11.0.0",
    "@angular/common": "~11.0.0",
    "@angular/compiler": "~11.0.0",
    "@angular/core": "~11.0.0",
    "@angular/forms": "~11.0.0",
    "@angular/platform-browser": "~11.0.0",
    "@angular/platform-browser-dynamic": "~11.0.0",
    "@angular/router": "~11.0.0",
    "@angular/service-worker": "~11.0.0",
    "rxjs": "~6.6.3",
    "tslib": "^2.0.3",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^11.0.0",
    "@angular-devkit/build-angular": "~0.1100.4",
    "@angular-eslint/builder": "1.0.0",
    "@angular-eslint/eslint-plugin": "1.0.0",
    "@angular-eslint/eslint-plugin-template": "1.0.0",
    "@angular-eslint/schematics": "^0.8.0-beta.7",
    "@angular-eslint/template-parser": "1.0.0",
    "@angular/cli": "~11.0.4",
    "@angular/compiler-cli": "~11.0.0",
    "@angular/localize": "^11.0.0",
    "@fullhuman/postcss-purgecss": "^3.1.3",
    "@types/jasmine": "~3.6.2",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^14.14.19",
    "@typescript-eslint/eslint-plugin": "4.11.1",
    "@typescript-eslint/parser": "4.11.1",
    "codelyzer": "^6.0.1",    
    "eslint-plugin-jsdoc": "30.7.13",
    "eslint": "^7.17.0",   
    "eslint-plugin-prefer-arrow": "1.2.2",
    "husky": "^4.3.6",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~6.0.0",
    "karma": "~5.2.3",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.5.0",
    "lint-staged": "^10.5.3",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^4.1.0",
    "postcss-scss": "^3.0.4",
    "protractor": "~7.0.0",
    "tailwindcss": "^1.9.6",
    "ts-node": "~9.1.1",
    "typescript": "~4.0.5",
    "webpack-bundle-analyzer": "^4.3.0"
  }
}
